<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="ar.com.hmu.controller.ListaUsuariosController">
    <TableView fx:id="usuariosTableView">
        <columns>
            <TableColumn fx:id="cuilColumn" text="CUIL" />
            <TableColumn fx:id="apellidosNombresColumn" text="Apellidos, Nombres" />
            <TableColumn fx:id="cargoNumeroColumn" text="Nro. de Cargo" />
            <TableColumn fx:id="servicioNombreColumn" text="Servicio" />
        </columns>
    </TableView>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
